{
  "name": "phapi/exception",
  "description": "Phapi Exceptions",
  "keywords": [
    "exceptions", "exception", "phapi"
  ],
  "version": "1.0.1",
  "minimum-stability": "stable",
  "license": "MIT",
  "homepage": "http://phapi.github.io",
  "authors": [
    {
      "name": "Peter Ahinko",
      "email": "peter@ahinko.se",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "phapi/contract": "1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Phapi\\": "src/Phapi/",
      "Phapi\\Exception\\": "src/Phapi/Exception/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Phapi\\Tests\\Exception\\": "tests/Phapi/Exception/"
    }
  }
}
